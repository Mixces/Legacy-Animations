{
	"schemaVersion": 1,
	"id": "legacyanimations",
	"version": "${version}",
	"name": "Legacy Animations",
	"description": "Brings back the legacy animations and visuals from versions before 1.9!",
	"authors": [
		"Mixces",
		"Microcontrollers",
		"ImToggle"
	],
	"contact": {
		"homepage": "https://github.com/Mixces",
		"sources": "https://github.com/Mixces/Legacy-Animations",
		"issues": "https://github.com/Mixces/Legacy-Animations/issues"
	},
	"license": "LGPLV3",
	"icon": "assets/legacyanimations/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.mixces.legacyanimations.LegacyAnimations"
		],
		"modmenu": [
			"com.mixces.legacyanimations.config.ModMenuIntegration"
		]
	},
	"mixins": [
      "legacyanimations.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "1.20.4",
